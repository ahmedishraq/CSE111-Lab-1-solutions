package task7;

/**
 *
 * @author 19301261
 */
import java.util.Scanner;

public class Task7 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int l = sc.nextInt();
        for (int i = 1; i <= n; i++) {
            for (int j = 1; j <= l; j++) {
                if (i == 1 || i == n) {
                    System.out.print(j);
                } else {
                    if (j == 1 || j == n) {
                        System.out.print(" ");
                    }
                }
            }
            System.out.println();
        }
    }
}
